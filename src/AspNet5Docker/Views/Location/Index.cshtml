
@model IEnumerable<AspNet5Docker.Models.Location>


<h1>All Locations</h1>

<ul>
    @foreach (var location in Model)
    {
        <li>@location.Name</li>
    }

</ul>

<form asp-controller="Location" asp-action="Add">
    <input type="text" name="newLocation" placeholder="add new location..." />
    <input type="submit" />
</form>


